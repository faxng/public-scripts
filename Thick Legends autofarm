-- place unlocks
rpart = game.Players.LocalPlayer.Character.HumanoidRootPart
rpart.MassPart:Destroy()
wait(0.5)
rpart.CFrame = CFrame.new(Vector3.new(792.022339, 1667.59656, 2188.81665))
wait(1)
rpart.CFrame = CFrame.new(Vector3.new(755.23407, 3245.05786, 2072.65942))
wait(1)
rpart.CFrame = CFrame.new(Vector3.new(686.256409, 5908.29688, 2048.33105))
wait(1)
rpart.CFrame = CFrame.new(Vector3.new(748.952209, 9126.31641, 2084.4353))
wait(1)
rpart.CFrame = CFrame.new(Vector3.new(688.492798, 12795.7314, 2045.11609))
wait(1)
rpart.CFrame = CFrame.new(Vector3.new(742.455139, 16551.8457, 2012.90918))
wait(1)
rpart.CFrame = CFrame.new(Vector3.new(753.364502, 21874.1367, 2073.98877))
wait(1)
rpart.CFrame = CFrame.new(Vector3.new(707.365295, 30255.0156, 2035.49048))
warn('ALL PLACES UNLOCKED!')
warn('ending your life for anti-cheat no-detect :D')
game.Players.LocalPlayer.Character.Humanoid.Health = 0

wait(2)

function warnhelp()
warn('-- starting the farm!')
warn('-- say r or /e r to rejoin and stop it!')
end

warnhelp()

game:GetService("Players").LocalPlayer.Chatted:Connect(function(msg)
if (string.lower(msg) == "r") then
place = game.PlaceId
service = game:GetService("TeleportService")
service:Teleport(place)
end
end)

game:GetService("Players").LocalPlayer.Chatted:Connect(function(msg)
if (string.lower(msg) == "/e r") then
place = game.PlaceId
service = game:GetService("TeleportService")
service:Teleport(place)
end
end)



-- loop

while wait(1) do
	r = game.ReplicatedStorage.Remotes.UseTool
	p = "Punch"
	s = "Stomp"
	f = "Food"
r:FireServer(p)
r:FireServer(s)
r:FireServer(f)
r:FireServer(p)
r:FireServer(s)
r:FireServer(f)
r:FireServer(p)
r:FireServer(s)
r:FireServer(f)
r:FireServer(p)
r:FireServer(s)
r:FireServer(f)
r:FireServer(p)
r:FireServer(s)
r:FireServer(f)
epic1()
wait(0.5)
epic2()
wait()
epic3()
wait()
epic4()
wait()
epic5()
wait()
epic6()
wait()
dab1()
wait()
dab2()
wait()
dab3()
wait()
dab4()
wait()
dab5()
wait()
dab6()
warnhelp()
end
