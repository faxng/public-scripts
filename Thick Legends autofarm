-- loop

while wait(1) do
	r = game.ReplicatedStorage.Remotes.UseTool
	p = "Punch"
	s = "Stomp"
	f = "Food"
	r:FireServer(p)
	r:FireServer(s)
	r:FireServer(f)
		epic1()
wait(0.5)
epic2()
wait()
epic3()
wait()
epic4()
wait()
epic5()
wait()
epic6()
wait()
dab1()
wait()
dab2()
wait()
dab3()
wait()
dab4()
wait()
dab5()
wait()
dab6()
end
