epicburgers = {"Burger6", "Burger5", "Burger4", "Burger3", "Burger2", "Burger"}
dabcoins = {"Coin6", "Coin5", "Coin4", "Coin3", "Coin2", "Coin"}

function epic1()
warn('done epic 1')
burg1 = game.Workspace.PickupItems:WaitForChild(epicburgers[1])
me = game.Players.LocalPlayer.Character.HumanoidRootPart
burg1.CFrame = me.CFrame
end

function epic2()
warn('done epic 2')
burg2 = game.Workspace.PickupItems:WaitForChild(epicburgers[2])
me = game.Players.LocalPlayer.Character.HumanoidRootPart
