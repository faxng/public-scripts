game:GetService("StarterGui"):SetCore("SendNotification",{
Title = "Thick Legends",
Text = "Take a few seconds to read the changelog (F9)",
Duration = 10,
})

warn('changelog: V1')
warn('-- made the script')
warn('-- added commands r (/e r is supported) to rejoin and stop the autofarm.')
warn('added protection so you dont get detected using it!')
warn('changelog: end')

wait(5)

epicburgers = {"Burger6", "Burger5", "Burger4", "Burger3", "Burger2", "Burger"}
dabcoins = {"Coin6", "Coin5", "Coin4", "Coin3", "Coin2", "Coin"}

function epic1()
print('done epic 1')
burg1 = game.Workspace.PickupItems:WaitForChild(epicburgers[1])
me = game.Players.LocalPlayer.Character.HumanoidRootPart
burg1.CFrame = me.CFrame
end

function epic2()
print('done epic 2')
burg2 = game.Workspace.PickupItems:WaitForChild(epicburgers[2])
