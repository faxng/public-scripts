togetgun = "RayGun"
path = game.Workspace.AREA51
path2 = game.Workspace.Spawns.SpawnLocation
var1 = " Giver"
myframe = game.Players.LocalPlayer.Character.HumanoidRootPart
complete = path[togetgun][togetgun..var1]["PUT THE WEAPON IN THIS BRICK"]

myframe.CFrame = complete.CFrame
wait(1)
myframe.CFrame = path2.CFrame * CFrame.new(myframe.Size.X/100, 10, 0)

wait(3)
warn('teleporting the killers')
for i,v in pairs(game.Workspace.Killers:GetDescendants()) do
	if v.Name == "HumanoidRootPart" then
		v.Anchored = true
epic = game.Players.LocalPlayer.Character.HumanoidRootPart
v.CFrame = epic.CFrame * CFrame.new(v.Size.X/100, 7, 0)
	end
	end
